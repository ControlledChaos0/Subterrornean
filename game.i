void updateBullet(BULLET* bullet) {
    if (bullet -> active != 0) {
        bullet->oldCol = bullet->col;
        bullet->oldRow = bullet->row;
        bullet->col += bullet->horVelocity;
        bullet->row += bullet->verVelocity;
        if (bullet->row <= 7 || bullet->row >= 160 - 7 || bullet->col <= 49
            || bullet->col >= 188) {
                bullet->active = 0;
                bullet->horVelocity = 0;
                bullet->verVelocity = 0;
                bullet->erased = 1;
            }
    }
}

void fireBullet() {
    for (int i = 0; i < (bulletCount); i++) {
        if (bullets[i].active == 0) {
            switch (player.index){
            case 0:
                bullets[i].horVelocity = -2;
                bullets[i].row = player.row + 3;
                bullets[i].col = player.col;
                break;
            case 1:
                bullets[i].horVelocity = 2;
                bullets[i].row = player.row + 3;
                bullets[i].col = player.col + 4;
                break;
            case 2:
                bullets[i].verVelocity = 2;
                bullets[i].row = player.row + 3;
                bullets[i].col = player.col + 2;
                break;
            case 3:
                bullets[i].verVelocity = -2;
                bullets[i].row = player.row + 3;
                bullets[i].col = player.col + 2;
                break;
            }
            bullets[i].active = 1;
            break;
        }
    }
}

